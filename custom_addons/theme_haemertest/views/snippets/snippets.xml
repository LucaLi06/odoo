<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">Haemers</div>
                <div class="o_panel_body">
                    <t t-snippet="theme_haemertest.home_snippet" t-thumbnail="/theme_haemertest/static/src/img/ino.png">
                        <keywords>tech, innovation, scalable app, references</keywords>
                    </t>
                    <t t-snippet="theme_haemertest.display_partner" t-thumbnail="/theme_haemertest/static/src/img/partner.png">
                    <keywords>partner</keywords>
                </t>
                </div>
            </div>
        </xpath>
    </template>
</odoo>